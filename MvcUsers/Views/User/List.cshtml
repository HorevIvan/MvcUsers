@model IEnumerable<MvcUsers.Models.User>

@{
    ViewBag.Title = "Список пользователей";
}

<h2>@ViewBag.Title</h2>

@Html.ActionLink("Добавить пользователя", "Registration")

<ul>
    @foreach (var user in Model)
    {
        <li>
            @user.Name [@Html.ActionLink("Просмотр", "Info", new { Number = user.Number })]
        </li>
    }
</ul>